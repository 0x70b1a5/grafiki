<%= content_for :title do %> | browse<% end %>
<% @bounties.each_with_index do |bounty,ind| %>
<% break if ind > 100 %><%# TODO: PAGINATE REMAINING ARTWORKS %>
<% next if bounty.artist %>
<div class="in-float" style="height:auto">
<div id="bounty<%=bounty.id%>"></div>
</div>
<% end %>
<script>
$( document ).ready(function(){
  Array(100).fill().forEach(function(val,i){
    $.get ( '/bounties/'+i+'.txt' ).then(function(rendered){
      $( "#bounty"+i ).html( rendered );
    });
  });
});
</script>
