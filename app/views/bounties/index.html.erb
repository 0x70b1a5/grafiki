<%= content_for :title do %> | bounties<% end %>
<% paginator = params[:start].to_i || 0
limit = 20
if @bounties.is_a? Array
  bounties = @bounties
else
  bounties = @bounties.where("id>? AND id<? AND pic==?",
    paginator,
    limit+paginator,
    "")
end %>

<%= react_component("BountyIndexPage", {
  bounties: bounties,
  items: "bounties",
  route: "/bounties",
  paginator: paginator,
  limit: limit
  }) %>
